{
	"$schema": "https://github.com/glzr-io/zebar/raw/v2.1.0/resources/widget-schema.json",
	"htmlPath": "./clear-starter.html",
	"zOrder": "bottom_most",
	"shownInTaskbar": false,
	"focused": false,
	"resizable": false,
	"transparent": true,
	"presets": [
		{
			"name": "default",
			"anchor": "top_left",
			"offsetX": "0px",
			"offsetY": "0px",
			"width": "100%",
			"height": "50px",
			"monitorSelection": {
				"type": "all"
			}
		}
	]
}
